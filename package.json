{
  "name": "library_management_system",
  "version": "1.0.1",
  "description": "This Back-End project is a library book-rental system. It is a REST API that allows you to manage books and users. It is developed with Express.js, Sequelize, and MySQL.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "lint": "eslint .",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.5.2",
    "sequelize": "^6.32.1"
  }
}
